<odoo>
  <data>

     <!-- Top menu item -->

    <menuitem id="student_odoo_moodle_root" name="Academy" sequence="44" web_icon="moodle_odoo_int,static/description/icon2.png"/>
    <menuitem id="student_odoo_moodle_students" name="Students" parent="student_odoo_moodle_root"
        sequence="1"/>

    <record id="student_odoo_moodle_view_form" model="ir.ui.view">
        <field name="name">student.odoo.moodle.form</field>
        <field name="model">student.odoo.moodle</field>
        <field name="arch" type="xml">
            <form>
              <field name="name"/>
            </form>
        </field>
    </record>

    <record id="student_odoo_moodle_view_tree" model="ir.ui.view">
        <field name="name">student.odoo.moodle.tree</field>
        <field name="model">student.odoo.moodle</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
            </tree>
        </field>
    </record>

    <!-- actions opening views on models -->
    <record model="ir.actions.act_window" id="student_odoo_moodle_action_window">
      <field name="name">student.odoo.moodle window</field>
      <field name="res_model">student.odoo.moodle</field>
    </record>

    <menuitem
        id="student_odoo_moodle_menu_action"
        action="student_odoo_moodle_action_window"
        parent="student_odoo_moodle_students"
        sequence="50"/>

  </data>
</odoo>